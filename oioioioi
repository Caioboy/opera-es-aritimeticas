
public class MyClass {
  public static void main(String args[]) {
    

    int num1, num2, total; 
    
    num1 = 30;
    num2 = 3;

    
    total = num1 + num2;
    System.out.println(total);
    total = num1 - num2;
    System.out.println(total);
    total = num1*num2;
    System.out.println(total);
    total = num1/num2;
    System.out.println(total); 
    
    
 
  
    
    
     }
